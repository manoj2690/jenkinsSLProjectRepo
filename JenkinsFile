libraries{

	lib('sharedlibrepo/sharedlibs')
}

pipeline {
	agent any
	
	stages {
		stage('Build') {
			steps{
				echo  'Build = calling shared lib method'
				
				 def command = 'java -version'
				
				echo  '>>>>>> shell = $command'
				
				build(command)
				
				echo 'end build stage'
			}
		}
	}
}