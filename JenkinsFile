libraries{

	lib('sharedlibrepo/sharedlibs')
}

pipeline {
	agent any
	
	stages {
		stage('Build') {
			
			println 'Build = calling shared lib method'
			
			def command = 'mvn --help'
			
			println '>>>>>> shell = ' + '$command'
			
			build(command)
			
			println 'end build stage'
		}
	}
}